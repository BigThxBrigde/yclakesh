<div class="row col-md-8" style="margin: 20px 10px">
    <% if(operation=='generate'){ %>

    <div class="box box-success">
        <div class="box-header with-border">
            <h2 class="box-title">生成二维码数据</h3>
        </div>
        <form class="form-horizontal" role="form">

            <div class="box-body">


                <div class="form-group">
                    <label class="col-xs-2 control-label" for="ds_name">开始序列号</label>
                    <div class="col-xs-3">
                        <input class="form-control" id="ds_name" type="text" placeholder="<%=startSerialId%>"
                            disabled />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-xs-2 control-label" for="ds_username">生成个数</label>
                    <div class="col-xs-3">
                        <input class="form-control" id="ds_username" type="text" placeholder="root" />
                    </div>

                </div>
                <div class="form-group">

                    <label class="col-xs-2 control-label" for="ds_password">会员</label>
                    <div class="col-xs-3">
                        <select class="form-control select2 select2-hidden-accessible" style="width: 100%;"
                            data-select2-id="1" tabindex="-1" aria-hidden="true">
                            <option selected="selected" data-select2-id="3">Alabama</option>
                            <option data-select2-id="37">Alaska</option>
                            <option data-select2-id="38">California</option>
                            <option data-select2-id="39">Delaware</option>
                            <option data-select2-id="40">Tennessee</option>
                            <option data-select2-id="41">Texas</option>
                            <option data-select2-id="42">Washington</option>
                        </select>
                        <span class="select2 select2-container select2-container--default" dir="ltr" data-select2-id="2"
                            style="width: 100%;"><span class="selection"><span
                                    class="select2-selection select2-selection--single" role="combobox"
                                    aria-haspopup="true" aria-expanded="false" tabindex="0"
                                    aria-labelledby="select2-s1yq-container"><span class="select2-selection__rendered"
                                        id="select2-s1yq-container" role="textbox" aria-readonly="true"
                                        title="Alabama">Alabama</span><span class="select2-selection__arrow"
                                        role="presentation"><b role="presentation"></b></span></span></span><span
                                class="dropdown-wrapper" aria-hidden="true"></span></span>
                    </div>
                </div>

            </div>

            <div class="box-footer">
                <div class="form-group">
                    <label class="col-xs-3"></label>
                    <button type="button" style="margin-left:-10px" class="btn btn-success col-xs-2">生成</button>

                </div>
            </div>
        </form>
        <!-- /.box-body -->
    </div>
    <% } %>
</div>
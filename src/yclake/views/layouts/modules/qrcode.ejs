<div>
    <% if(operation=='generate'){ %>
    <div class="box box-success">
        <div class="box-header with-border">
            <h4 class="box-title">生成</h3>
        </div>
        <div class="box-body">
            <div class="row">
                <span><b>开始序列号:</b></span><span><%=startSerialId%></span>
            </div>
            <div class="row">
                <form class="form-inline">
                    <div class="form-group col-xs-3">
                        <label for="serialCount">生成个数</label>
                        <input type="text" class="form-control" id="serialCount" placeholder="请输入生成个数">
                    </div>
                    <div class="form-group col-xs-3">
                        <label for="exampleInputEmail2">会员</label>
                        <input type="email" class="form-control" id="exampleInputEmail2"
                            placeholder="jane.doe@example.com">
                    </div>
                    <button type="button" class="btn btn-success">生成</button>
                </form>
            </div>
        </div>
    </div>
    <% } %>
</div>